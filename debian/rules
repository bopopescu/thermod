#!/usr/bin/make -f

#export DH_VERBOSE = 1
export PYBUILD_NAME = thermod

%:
	dh $@ --with python3,apache2 --buildsystem=pybuild

override_dh_auto_build:
	dh_auto_build
	doxygen

override_dh_auto_clean:
	dh_auto_clean
	rm -fr doc/

